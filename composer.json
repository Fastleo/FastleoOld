{
    "name": "camanru/fastleo",
    "type": "project",
    "version": "0.1.20",
    "authors": [
        {
            "name": "Roman Zakharov",
            "email": "camanru@ya.ru"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/dbal": "^2.7",
        "league/csv": "^9.1",
        "illuminate/support": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Camanru\\Fastleo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Camanru\\Fastleo\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Camanru\\Fastleo\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=fastleo --force"
        ],
        "post-autoload-dump": [
            "@php artisan vendor:publish --tag=fastleo --force"
        ]
    }
}
