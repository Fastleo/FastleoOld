{
    "name": "camanru/fastleo",
    "type": "project",
    "version": "0.1.6",
    "authors": [
        {
            "name": "Roman Zakharov",
            "email": "camanru@ya.ru"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/dbal": "^2.7",
        "illuminate/support": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "Camanru\\Fastleo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Camanru\\Fastleo\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --tag=fastleo --force"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --tag=fastleo --force"
        ]
    }
}
